{
  "name": "unocss-preset-vuetify",
  "version": "0.1.0",
  "description": "UnoCSS preset that replicates Vuetify utility classes",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "pnpm build && bumpp -r"
  },
  "peerDependencies": {
    "unocss": ">=0.50.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-vuetify": "^2.0.2",
    "conventional-github-releaser": "^3.1.5",
    "eslint": "^9.39.1",
    "eslint-config-vuetify": "^4.3.3",
    "typescript": "^5.7.2",
    "unbuild": "^3.2.0",
    "unocss": "^0.65.1"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "vuetify",
    "utility-classes",
    "css"
  ],
  "author": "Vuetify",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuetifyjs/unocss-preset-vuetify"
  },
  "packageManager": "pnpm@10.11.0"
}
